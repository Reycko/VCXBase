  <Target Name="GetGitInfo" BeforeTargets="InitializeBuildStatus">
    <!-- HACK: Couldn't get directly parsing console output to work -->
    <PropertyGroup>
      <TempGitCommitFile>$(MSBuildProjectDirectory)\git_commit.txt</TempGitCommitFile>
      <TempGitShortCommitFile>$(MSBuildProjectDirectory)\git_short_commit.txt</TempGitShortCommitFile>
    </PropertyGroup>
    <Exec Command="git rev-parse HEAD &gt; &quot;$(TempGitCommitFile)&quot;" />
    <Exec Command="git rev-parse --short HEAD &gt; &quot;$(TempGitShortCommitFile)&quot;" />
    <ReadLinesFromFile File="$(TempGitCommitFile)">
      <Output TaskParameter="Lines" ItemName="GitCommit" />
    </ReadLinesFromFile>
    <ReadLinesFromFile File="$(TempGitShortCommitFile)">
      <Output TaskParameter="Lines" ItemName="GitShortCommit" />
    </ReadLinesFromFile>
    <!-- Cleanup -->
    <Delete Files="$(TempGitCommitFile)" />
    <Delete Files="$(TempGitShortCommitFile)" />
    <ItemGroup>
      <ClCompile>
        <PreprocessorDefinitions>GIT_COMMIT="@(GitCommit)";GIT_SHORT_COMMIT="@(GitShortCommit)";$(PreprocessorDefinitions)</PreprocessorDefinitions>
      </ClCompile>
    </ItemGroup>
  </Target>
